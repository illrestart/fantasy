syntax enable
set ts=4
set sw=4
set sts=4
set nonu
set cindent
set et
set number
set wrap
set ruler
set hlsearch
set backspace=indent,eol,start
set list
set lcs=tab:>-,trail:-
set incsearch
set wrap
set noexpandtab
"set tenc=utf8
set nobackup
"set enc=gbk
"set fenc=gbk
"set fileencodings=gb18030,utf8
color desert
colorscheme elflord

let Tlist_Show_One_file = 1
let Tlist_Exit_OnlyWindow = 1
let Tlist_Use_Right_Window = 1
let Tlist_Auto_Open = 1
map <silent> <F8> :TlistToggle<CR>

"filetype plugin indent on

"set fileencodings=gb2312,ucs-bom,utf-8,gbk,cp936,big5,euc-jp,euc-kr,latin1
"set fileencodings=ucs-bom,utf-8,gbk,cp936,gb18030,big5,euc-jp,euc-kr,latin1
"set termencoding=gb2312
"set encoding=gb2312
"set fileencoding=gb2312

"set encoding=utf-8
"set termencoding=utf-8
"set fileencodings=utf-8
set fileformats=unix

